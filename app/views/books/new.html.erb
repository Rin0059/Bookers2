<h1>画像投稿フォーム</h1>

<%= form_for @book do |form| %>
  <%= form.attachment_field :profile_image  %>
<% end %>


<%= form_with model: @user, local:true do |f| %>
  <h4>画像</h4>
  <%= f.attachment_field :image %>
  <h4>ショップ名</h4>
  <%= f.text_field :title %>
  <h4>説明</h4>
  <%= f.text_area :body %>
  <%= f.submit '投稿' %>
<% end %>